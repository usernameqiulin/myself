{
  "name": "@bone/bone-mobile",
  "version": "2.2.4",
  "description": "bone mobile framework",
  "main": "lib",
  "scripts": {
    "lint": "eslint src/",
    "fix": "eslint --fix src",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lib": "rm -rf lib && babel src --out-dir lib",
    "prepublishOnly": "npm run build",
    "test": "jest src/* --watch",
    "test-cov": "jest --coverage",
    "test-debug": "node --debug-brk ./node_modules/.bin/jest src/libs/router.test.js --runInBand"
  },
  "jest": {
    "testURL": "http://open.aliplus.com/bone/",
    "verbose": true,
    "preset": "react-native",
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.alibaba-inc.com/bone/bone-mobile-sdk.git"
  },
  "keywords": [
    "bone",
    "mobile",
    "framework",
    "react",
    "react-native"
  ],
  "author": "amoschen",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15.4": "^1.0.5",
    "eslint-config-ali": "^2.0.3",
    "eslint-plugin-jest": "^21.6.1",
    "jest": "^20.0.4",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "15"
  },
  "dependencies": {
    "@bone/bone-mobile-reset": "^0.1.5",
    "@bone/bone-mv": "^0.1.13",
    "@bone/event-emitter": "^0.1.3",
    "@bone/logger": "^0.2.2",
    "core-js": "^2.5.1",
    "invariant": "^2.2.2",
    "qs": "^6.5.1",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-native": "0.41.2",
    "react-native-web": "^0.0.130",
    "react-router": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "type-detect": "^4.0.5"
  },
  "bnpm": {
    "mode": "yarn"
  }
}
