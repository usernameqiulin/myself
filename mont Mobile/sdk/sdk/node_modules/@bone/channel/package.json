{
  "name": "@bone/channel",
  "version": "0.1.13",
  "description": "channel为基于mqtt的长连接通信协议封装，可直接使用完整库，也可以单独引用接口层（使用Native SDK时）或实现层（模拟Native SDK bridge）",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run buildLib; npm run buildDist; npm run doc",
    "buildLib": "babel src --out-dir lib",
    "buildDist": "webpack --env=prod --progress --profile --colors",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "doc": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+http://gitlab.alibaba-inc.com/bone/channel.git"
  },
  "keywords": [
    "mqtt",
    "websocket"
  ],
  "author": "amos",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "esdoc": "^1.0.4",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@bone/event-emitter": "^0.1.4",
    "@bone/logger": "^0.1.6",
    "@bone/paho-client": "^0.1.0"
  }
}
